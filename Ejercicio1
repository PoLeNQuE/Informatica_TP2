#include <iostream>
#include <math.h>
using namespace std;

int main() {
int a,b,c,x1,x2,d;
	printf ("ingresa un valor para el coeficiente a: ");
	scanf ("%d",&a);
	printf ("\n\ningrese un valor para el coeficiente b: ");
	scanf ("%d",&b);
	printf ("\n\ningresa un valor para el coeficiente c: ");
	scanf ("%d",&c);
	
	d = pow(b,2)-4*a*c ;
	
	if (d >= 0){
		x1 = -b + sqrt(d)/(2*a);
		x2 = -b - sqrt(d)/(2*a);
		printf ("x1 = %d", x1);
		printf ("\nx2 = %d", x2);
		
	}
	else{
		printf ("\nRaices complejas");
	}
	return 0;
}

